@{
    Layout = "_Layout";
}

@section Sheets {
    @RenderSection("Sheets", required: false)
}

@RenderBody()

@section Scripts {
    <script type="text/javascript">$(".nav-link").removeClass("active");
        $("#nav-graph").addClass("active");</script>
    <environment include="Development">
        <script src="~/js/d3.min.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/d3/5.9.2/d3.min.js" crossorigin="anonymous"></script>
    </environment>
    @RenderSection("Scripts", required: false)
}