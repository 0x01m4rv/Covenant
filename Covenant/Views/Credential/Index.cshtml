@model IEnumerable<Covenant.API.Models.CapturedCredential>
@{
    Layout = "_CredentialLayout";
    ViewData["Title"] = "Credentials";
}
<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-4 border-bottom">
    <h1 class="h2">Credentials</h1>
</div>

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center">
    <h4 class="h5">Password Credentials</h4>
</div>

<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>Domain</th>
                <th>Username</th>
                <th>ServiceName</th>
                <th>Password</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.PasswordCredentials) {
            <tr>
                <td>@item.Domain</td>
                <td>@item.Username</td>
                <td>@item.ServiceName</td>
                <td>@item.Password</td>
            </tr>
            }
        </tbody>
    </table>
</div>

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center">
    <h4 class="h5">Hash Credentials</h4>
</div>

<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>Domain</th>
                <th>Username</th>
                <th>ServiceName</th>
                <th>HashCredentialType</th>
                <th>Hash</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.HashCredentials) {
            <tr>
                <td>@item.Domain</td>
                <td>@item.Username</td>
                <td>@item.ServiceName</td>
                <td>@item.HashCredentialType</td>
                <td>@item.Hash</td>
            </tr>
            }
        </tbody>
    </table>
</div>

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center">
    <h4 class="h5">Ticket Credentials</h4>
</div>

<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>Domain</th>
                <th>Username</th>
                <th>ServiceName</th>
                <th>TicketCredentialType</th>
                <th>Ticket</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.TicketCredentials) {
            <tr>
                <td>@item.Domain</td>
                <td>@item.Username</td>
                <td>@item.ServiceName</td>
                <td>@item.TicketCredentialType</td>
                <td>@item.Ticket</td>
            </tr>
            }
        </tbody>
    </table>
</div>